<div class="lists">
  <% @lists.each do |list| %>
    <%= link_to list.name, list_path(list), class: "list", style: "background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('#{cl_image_path list.photo.key, crop: :fill}')" %>
  <% end %>
  <%= link_to "+ Add a list", new_list_path, class: "add" %>
</div>
